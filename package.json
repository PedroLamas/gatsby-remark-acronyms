{
  "name": "gatsby-remark-acronyms",
  "version": "1.0.3",
  "description": "Gatsby Remark plugin to show description tooltips for acronyms found in markdown text",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "format": "eslint --fix src",
    "build": "babel src --out-dir . --ignore **/__tests__",
    "test": "jest",
    "prepublishOnly": "cross-env NODE_ENV=production yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PedroLamas/gatsby-remark-acronyms.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "remark",
    "markdown",
    "acronym"
  ],
  "author": "Pedro Lamas <pedrolamas@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PedroLamas/gatsby-remark-acronyms/issues"
  },
  "homepage": "https://github.com/PedroLamas/gatsby-remark-acronyms#readme",
  "dependencies": {
    "unist-util-visit": "^1.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "cross-env": "^5.2.1",
    "eslint": "^6.3.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "remark": "^11.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  }
}
